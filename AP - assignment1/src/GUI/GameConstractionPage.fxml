<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="385.0" prefWidth="609.0" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.TableController">
   <children>
      <TableView fx:id="candidatesTable" layoutX="14.0" layoutY="41.0" prefHeight="200.0" prefWidth="284.0">
        <columns>
          <TableColumn fx:id="iType1" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="iName1" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="iAthleteType1" prefWidth="133.0" text="Type" />
        </columns>
      </TableView>
      <TableView fx:id="participantsTable" layoutX="313.0" layoutY="41.0" prefHeight="200.0" prefWidth="284.0">
        <columns>
            <TableColumn fx:id="iType2" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="iName2" prefWidth="75.0" text="Name" />
          <TableColumn fx:id="iAthleteType2" prefWidth="133.0" text="Type" />
        </columns>
      </TableView>
      <Button layoutX="38.0" layoutY="253.0" mnemonicParsing="false" onAction="#addAction" prefHeight="27.0" prefWidth="102.0" text="Add Athletes" />
      <Button layoutX="172.0" layoutY="253.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="102.0" text="Add Referees" />
      <Button fx:id="addDelete" layoutX="404.0" layoutY="253.0" mnemonicParsing="false" onAction="#delectAction" prefHeight="27.0" prefWidth="102.0" text="Delete" />
      <Button fx:id="confirm" layoutX="253.0" layoutY="331.0" mnemonicParsing="false" onAction="#confirmAction" text="Confirm">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Label layoutX="14.0" layoutY="14.0" prefHeight="17.0" prefWidth="383.0" text="Notice: One game only contain one referees and 4-8 athlests!" />
   </children>
</AnchorPane>
